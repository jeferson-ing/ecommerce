<p id="notice"><%= notice %></p>
<div class="card">
  <div class="card-header bg-success text-white">
    <center><h1>Sellers</h1></center>
  </div>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Raiting</th>
      <th>Phone</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.user_id %></td>
        <td><%= seller.raiting %></td>
        <td><%= seller.phone %></td>
        <td><%= seller.product_id %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<button type="button" class="btn btn-primary"><a style="color:#FFFFFF"<%= link_to 'New Seller', new_seller_path %>
</button>
</div>